#定义变量
snake=[[0,0],[10,0],[20,0],[30,0],[40,0],[50,0]]
apple_x=randrange(-20,18)*10
apple_y=randrange(-19,19)*10
aim_x=0
aim_y=10

#定义函数
def change(x,y):
    global aim_x,aim_y
    aim_x=x
    aim_y=y

